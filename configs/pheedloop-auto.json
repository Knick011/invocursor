    "name": "PheedLoop",
    "description": "Event management software"
  },
  "pages": {
    "overview": {
      "description": "Event Overview",
      "elements": {
        "#event-name": {
          "type": "text",
          "label": "Event Name",
          "synonyms": [
            "event title",
            "name of the event"
          ]
        },
        "#event-description": {
          "type": "text",
          "label": "Description",
          "synonyms": [
            "event details",
            "event info"
